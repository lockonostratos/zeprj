<%= simple_form_for(@product_summary) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :product_code %>

    <%= f.input :skull_id %>

    <%= f.input :warehouse_id %>

    <%= f.input :name %>

    <%= f.input :qualtily %>

    <%= f.input :price %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
