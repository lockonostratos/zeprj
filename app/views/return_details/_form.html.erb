<%= simple_form_for(@return_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :return_id %>

    <%= f.input :product_id %>

    <%= f.input :current_quality %>

    <%= f.input :return_quality %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
