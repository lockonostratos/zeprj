<%= simple_form_for(@return_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :return_id %>

    <%= f.input :return_product_id %>

    <%= f.input :return_quality %>

    <%= f.input :type_return %>

    <%= f.input :product_id %>

    <%= f.input :price %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
