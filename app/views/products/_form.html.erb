<%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :product_code %>
    <%= f.input :skull_code %>
    <%= f.input :provider_id %>
    <%= f.input :warehouse_id %>
    <%= f.input :import_id %>
    <%= f.input :name %>
    <%= f.input :qualtiy %>
    <%= f.input :price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
