<%= simple_form_for(@merchant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :id %>

    <%= f.input :headquater_id %>

    <%= f.input :owner_id %>

    <%= f.input :name %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
