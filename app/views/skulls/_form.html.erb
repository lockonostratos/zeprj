<%= simple_form_for(@skull) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :merchant_id %>

    <%= f.input :merchant_account_id %>

    <%= f.input :skull_code %>

    <%= f.input :description %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
