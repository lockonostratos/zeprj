<%= simple_form_for(@delivery) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :order_id %>

    <%= f.input :merchant_account_id %>

    <%= f.input :status %>

    <%= f.input :delivery_date %>

    <%= f.input :transportation_fee %>

    <%= f.input :comment %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
