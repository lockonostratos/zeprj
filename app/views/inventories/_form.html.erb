<%= simple_form_for(@inventory) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :warehouse_id %>

    <%= f.input :merchant_account_id %>

    <%= f.input :decription %>

    <%= f.input :success %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
