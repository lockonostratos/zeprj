<%= simple_form_for(@merchant_account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :account_id %>

    <%= f.input :merchant_id %>

    <%= f.input :branch_id %>

    <%= f.input :current_warehouse_id %>

    <%= f.input :role_id %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
